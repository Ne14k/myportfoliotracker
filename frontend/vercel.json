{
  "version": 2,
  "name": "portfolio-risk-analyzer",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/build/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "devCommand": "npm start",
  "installCommand": "npm install"
}